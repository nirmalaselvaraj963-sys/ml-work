          },
          "metadata": {},
          "execution_count": 157
        }
      ],
      "source": [
        "from sklearn.model_selection import GridSearchCV, cross_val_predict\n",
        "\n",
        "# Define parameter grid (smaller for speed)\n",
        "param_grid = {\n",
        "    'C': [0.1, 1, 10],\n",
        "    'kernel': ['linear', 'rbf'],\n",
        "    'gamma': ['scale', 'auto'],\n",
        "    'class_weight': [None, 'balanced']\n",
        "}\n",
        "\n",
        "grid = GridSearchCV(SVC(), param_grid, refit = True, verbose = 3,n_jobs=-1,scoring='f1_weighted')\n",
        "\n",
        "# fitting the model for grid search\n",
        "grid.fit(X,y)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "5039e0c0-ba8e-454f-9f1d-1bf2b1ad7e24",
      "metadata": {
        "id": "5039e0c0-ba8e-454f-9f1d-1bf2b1ad7e24"
      },
      "outputs": [],
      "source": [
        "# Cross-validated predictions (instead of X_test)\n",
        "y_pred = cross_val_predict(grid.best_estimator_, X, y, cv=5)"
      ]
    },
    {
